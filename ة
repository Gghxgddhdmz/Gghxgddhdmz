local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Al7FRAA,
   Icon = 0, -- Icon in Topbar. Can use Lucide Icons (string) or Roblox Image (number). 0 to use no icon (default).
   LoadingTitle = "I roberto",
   LoadingSubtitle = "by roberto",
   ShowText = "روبيرتو", -- عجبكم سكربت
   Theme = "Serenity", -- Check https://docs.sirius.menu/rayfield/configuration/themes

   ToggleUIKeybind = "K", -- The keybind to toggle the UI visibility (string like "K" or Enum.KeyCode)

   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false, -- Prevents Rayfield from warning when the script has a version mismatch with the interface

   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "Big Hub"
   },

   Discord = {
      Enabled = false, -- Prompt the user to join your Discord server if their executor supports it
      Invite = "noinvitelink", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ ABCD would be ABCD
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },

   KeySystem = false, -- Set this to true to use our key system
   KeySettings = {
      Title = "Untitled",
      Subtitle = "Key System",
      Note = "No method of obtaining the key is provided", -- Use this to tell the user how to get a key
      FileName = "Key", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
      SaveKey = true, -- The user's key will be saved, but if you change the key, they will be unable to use your script
      GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = {"Hello"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
   }
})

local Tab = Window:CreateTab("Tab Example", 4483362458) -- Title, Image

local Button = Tab:CreateButton({
   Name = "Button Example",
   Callback = function()
   -- سكربت X-Ray بسيط (LocalScript)

local Players = game:GetService("Players")
local player = Players.LocalPlayer
local workspace = game:GetService("Workspace")

-- دالة لجعل جزء شفاف للعميل فقط
local function makeXrayPart(part)
    if part:IsA("BasePart") then
        part.LocalTransparencyModifier = 0.5
    end
end

-- دالة لتطبيق X-Ray على جميع الأجزاء باستخدام الاستدعاء العودي
local function recurseForParts(object)
    -- إذا كان العنصر جزء (BasePart)، طبّق الشفافية
    makeXrayPart(object)
    -- إذا وجدنا لاعب (Humanoid)، توقف هنا لنمنع شفافية اللاعبين
    if object:FindFirstChildOfClass("Humanoid") then
        return
    end
    -- واصل البحث في الأطفال
    for _, child in pairs(object:GetChildren()) do
        recurseForParts(child)
    end
end

-- تنفيذ X-Ray لكل ما في workspace
recurseForParts(workspace)
print("X-Ray enabled: transparency applied locally to parts.")
   end,
})

local Button = Tab:CreateButton({
   Name = "Button Example",
   Callback = function()
   -- The function that takes place when the button is pressed
   end,
})

local Button = Tab:CreateButton({
   Name = "Button Example",
   Callback = function()
-- ⚡ سكربت لتقليل اللاغ وزيادة الأداء ⚡
-- Made by Hassan + ChatGPT

-- حذف التأثيرات الثقيلة
for _, v in pairs(game.Lighting:GetChildren()) do
    if v:IsA("BloomEffect") or v:IsA("BlurEffect") or v:IsA("SunRaysEffect") or v:IsA("DepthOfFieldEffect") or v:IsA("ColorCorrectionEffect") then
        v:Destroy()
    end
end

-- تعطيل الظلال
game.Lighting.GlobalShadows = false
game.Lighting.FogEnd = 1e9
game.Lighting.Brightness = 2

-- تعطيل تأثير المياه
workspace:FindFirstChildOfClass("Terrain").WaterWaveSize = 0
workspace:FindFirstChildOfClass("Terrain").WaterWaveSpeed = 0
workspace:FindFirstChildOfClass("Terrain").WaterReflectance = 0
workspace:FindFirstChildOfClass("Terrain").WaterTransparency = 0

-- تعطيل Particles
for _, v in pairs(workspace:GetDescendants()) do
    if v:IsA("ParticleEmitter") or v:IsA("Trail") then
        v.Lifetime = NumberRange.new(0)
    end
end

-- تعطيل Decals/Textures الثقيلة
for _, v in pairs(workspace:GetDescendants()) do
    if v:IsA("Decal") or v:IsA("Texture") then
        v.Transparency = 1
    end
end

-- تعطيل أصوات الخلفية
for _, v in pairs(workspace:GetDescendants()) do
    if v:IsA("Sound") then
        v.Playing = false
    end
end

-- تقليل الجودة
settings().Rendering.QualityLevel = Enum.QualityLevel.Level01

-- رسالة نجاح
print("✅ تم تفعيل سكربت Anti-Lag بنجاح!")
   end,
})

